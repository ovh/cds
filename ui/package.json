{
  "name": "ui-ng2",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "start": "ng serve -c development",
    "test": "ng lint && ng test --watch false --code-coverage",
    "test:model": "ng lint && ng test -c model --watch false --code-coverage",
    "test:shared": "ng lint && ng test -c shared --watch false --code-coverage",
    "test:views": "ng lint && ng test -c views --watch false --code-coverage",
    "test:service": "ng lint && ng test -c service --watch false --code-coverage",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.json -s",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.ts": "ng lint ui-ng2 --files"
    },
    "relative": true,
    "ignore": [
      "**/*.spec.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.2",
